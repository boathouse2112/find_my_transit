<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>com.markmurphyjrdev.FindMyTransit.CacheBackup</string>
		<key>ProgramArguments</key>
		<array>
			<string>zsh</string>
			<string>-c</string>
      <string>cd ~/Programs/find_my_transit; target/release/find_my_cache_backup</string>
    </array>
		<key>RunAtLoad</key>
		<true/>
    <key>WatchPaths</key>
    <array>
      <string>~/Library/Caches/com.apple.findmy.fmipcore/Items.data</string>
    </array>
	</dict>
</plist>
